
  

      <% if (latestblog && latestblog.length> 0) { %>

        <!--=== Sidebar Widget ===-->
        <div class="sidebar-widget sidebar-post-widget mb-40" data-aos="fade-up" data-aos-duration="1000">
          <h4 class="widget-title">Recent Posts</h4>
          <div class="widget-content">
              <ul class="recent-post-list">


                <% latestblog.forEach(item=> { %>
                  <li class="post-thumbnail-content mb-4">
                      <img src="<%= process.env.S3_BASE_URL + item.coverImage %>" alt=" <%= item.title %>">
                      <div class="post-title-date">
                          <span class="posted-on"><a href="/post/<%= item.seoDetails.slug %>"><%= item.postDate %></a></span>
                          <h5><a href="/post/<%= item.seoDetails.slug %>"><%= item.title %>/a></h5>
                          </div>
                      </li>
                      <% }) %>
                  
                  </ul>
              </div>
          </div>
  
          <% } %>