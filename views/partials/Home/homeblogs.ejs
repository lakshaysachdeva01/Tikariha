   <!-- BLOG
        ================================================== -->
        <% if (blogs && blogs.length > 0) { %>
      
                  <% blogs.forEach((item, index) => { %>
                  <div  onclick="window.location.href='/blog/<%= item.seoDetails.slug %>';">
                    
                              <img src="<%= process.env.S3_BASE_URL + item.banner.image %>" class="rounded-top" alt="<%= item.title %>">
                         
                                  <% if (item.category) { %>
                                      <a href="/blog/<%= item.seoDetails.slug %>" class="text-uppercase category-name fw-bold display-31"><%= item.category %></a>
                                  <% } else { %>
                                      <a href="/blog/<%= item.seoDetails.slug %>" class="text-uppercase fw-bold display-31">Blog</a>
                                  <% } %>
                                  <% if (item.postDate) { %>
                                      <span style="font-size: 14px; font-weight: 600;" class="category-name ms-2">â€¢ <%= item.postDate %></span>
                                  <% } %>
                              
                              <h4 class="mb-0"><a href="/blog/<%= item.seoDetails.slug %>"><%= item.title %></a></h4>
                      
                  </div>
                  <% }) %>
              
      <% } %>


